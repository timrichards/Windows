<Application x:Class="DoubleFile.App"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DoubleFile"
             xmlns:controls="clr-namespace:FirstFloor.ModernUI.Windows.Controls;assembly=FirstFloor.ModernUI"
            StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/FirstFloor.ModernUI;component/Assets/ModernUI.xaml" />
                <ResourceDictionary Source="/Double File;component/Assets/LocalBlankWindow.xaml" />
                <ResourceDictionary Source="/Double File;component/Assets/LocalMUIdark.xaml" />
                <ResourceDictionary Source="/Double File;component/Assets/Strings.xaml" />
                <ResourceDictionary Source="/Double File;component/Assets/LocalStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
