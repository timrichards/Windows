<local:LocalWindow x:Class="DoubleFile.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:DoubleFile"
        Title="Double File" Height="96" Width="254" Icon="/Double File;component/Resources/ic_people_black_18dp.png"
        Left="120" Top="20">
    <Grid Name="form_grid">
        <Menu IsMainMenu="True" Grid.ColumnSpan="999" Grid.RowSpan="999"/>
        <Button Content="View project" Name ="form_btnViewProject" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Grid.Column="0"/>
        <Button Content="Open project" Name ="form_btnOpenProject" Grid.Column="0" HorizontalAlignment="Left" Margin="80,0,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Save project" Name ="form_btnSaveProject" Grid.Column="0" HorizontalAlignment="Left" Margin="160,0,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="SearchDirLists" Name ="form_btnSearchDirLists" Grid.Column="0" HorizontalAlignment="Left" Margin="0,29,0,0" VerticalAlignment="Top" Width="90"/>
        <Button Content="Duplicate File Explorer" Name ="form_btnDuplicateFileExplorer" Grid.Column="0" HorizontalAlignment="Left" Margin="95,29,0,0" VerticalAlignment="Top" Width="140"/>
    </Grid>
</local:LocalWindow>
