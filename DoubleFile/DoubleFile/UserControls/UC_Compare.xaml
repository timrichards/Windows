<local:LocalUserControlBase x:Class="DoubleFile.UC_Compare"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:DoubleFile"
        LocalTitle="Compare">
    <Grid>
        <CheckBox Content="{Binding Folder1}" Command="{Binding Icmd_Pick1}"/>
        <CheckBox Content="{Binding Folder2}" Command="{Binding Icmd_Pick2}"/>
        <TextBlock Text="{Binding Results}"/>
    </Grid>
</local:LocalUserControlBase>
