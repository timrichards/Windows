<local:LocalUserControlBase x:Class="DoubleFile.UC_SolitaryHere"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:DoubleFile"
        LocalTitle="All solitary folders here">
    <local:UC_FolderList_Base x:Name="form_ucFolderList">
        <CheckBox IsChecked="{Binding SolitaryIsAllOneVol}" Command="{Binding Icmd_SolitaryIsAllOneVol}" Content="All One Vol: Solitary"/>
        <Button Command="{Binding Icmd_Export}" Content="Export" />
    </local:UC_FolderList_Base>
</local:LocalUserControlBase>
