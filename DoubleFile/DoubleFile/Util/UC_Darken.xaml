<local:UC_ContentPresenter x:Class="DoubleFile.UC_Darken"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:DoubleFile"
        Grid.RowSpan="999" Grid.ColumnSpan="999" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
        Visibility="Collapsed">
    <Grid>
        <Grid.Background>
            <SolidColorBrush Color="Black" Opacity=".4"/>
        </Grid.Background>
        <local:UC_Progress x:Name="ProgressCtl" HorizontalAlignment="Stretch" VerticalAlignment="Center"/>
        <local:UC_Messagebox x:Name="MessageboxCtl" HorizontalAlignment="Stretch" VerticalAlignment="Center"/>
    </Grid>
</local:UC_ContentPresenter>
